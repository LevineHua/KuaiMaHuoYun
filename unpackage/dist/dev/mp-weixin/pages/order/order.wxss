.uni-swiper-tab{
	height: 74rpx;
	line-height: 0;
	border-bottom: solid 1px #EEEEEE;
}
.swiper-tab-list{
	width: 25%;
	font-size: 24.17rpx;
	color: #818181;
	height: 72rpx;
	line-height: 72rpx;
}
.swiper-tab-list.active{
	color: #139CFF;
	border-bottom: solid 2px #139CFF;
}
.swiper-box{
	height: calc(100% - 76rpx);
}
.list{
	height: 100%;
}


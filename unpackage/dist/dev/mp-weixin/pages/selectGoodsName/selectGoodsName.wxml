<view class="select-car-wrap" bind:__l="__l"><block wx:if="{{historyName.length!=0}}"><view class="car_title">历史记录<text data-event-opts="{{[['tap',[['clearHistory',['$event']]]]]}}" class="clear_history" bindtap="__e">清除</text></view></block><view class="car_list"><block wx:for="{{historyName}}" wx:for-item="hc" wx:for-index="h_index" wx:key="h_index"><view data-event-opts="{{[['tap',[['onSelect',['$0'],[[['historyName','',h_index]]]]]]]}}" class="{{['car_item car_item_hc',(goodsName===hc&&selfGoodsName=='')?'active':'']}}" bindtap="__e">{{hc}}</view></block></view><view class="car_title">货物名称</view><view class="car_list"><block wx:for="{{carType}}" wx:for-item="ct" wx:for-index="t_index" wx:key="t_index"><view data-event-opts="{{[['tap',[['onSelect',['$0'],[[['carType','',t_index,'name']]]]]]]}}" class="{{['car_item',(goodsName===ct.name&&selfGoodsName=='')?'active':'']}}" bindtap="__e">{{ct.name}}</view></block></view><view class="car_title">自定义货物名称</view><view class="self_input"><input class="{{['self_type',(selfGoodsName!='')?'active':'']}}" type="text" placeholder-class="self_pla" placeholder="自定义货物名称" data-event-opts="{{[['input',[['__set_model',['$0','selfGoodsName','$event',[]],['']]]]]}}" value="{{selfGoodsName}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['selected',['$event']]]]]}}" class="select_btn" bindtap="__e">确定</view></view>
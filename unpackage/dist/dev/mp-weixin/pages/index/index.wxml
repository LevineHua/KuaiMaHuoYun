<view class="page" bind:__l="__l"><swiper class="swiper" indicator-dots="true" autoplay="true" interval="5000" indicator-active-color="#75c5ff" duration="500" current="0" circular="true"><block wx:for="{{bannerList}}" wx:for-item="bl" wx:for-index="__i0__" wx:key="id"><swiper-item><image class="banner" src="{{'https://cc.jiangxiruqi.com'+bl.image}}"></image></swiper-item></block></swiper><view class="select_infos"><km-title title="请选择地区"></km-title><km-select title="选择出发地" icon="../static/address1.png" error="{{setoutCityIsError}}" text="{{setoutCity}}" navigateTo="../writeAddress/writeAddress?type=setout"></km-select><km-select title="选择目的地" icon="../static/address2.png" error="{{endCityIsError}}" text="{{endCity}}" navigateTo="../writeAddress/writeAddress?type=end"></km-select><km-title title="请选择车辆信息"></km-title><km-select title="选择车辆信息" icon="../static/car.png" error="{{carInfoIsError}}" text="{{carInfo}}" navigateTo="../selectCar/selectCar"></km-select><block wx:if="{{userType==1}}"><view data-event-opts="{{[['tap',[['searchOrder']]]]}}" class="{{['select_btn',(examine!=1)?'select_disable':'']}}" bindtap="__e">查询</view></block><block wx:if="{{userType==0}}"><km-input title="填写需要车辆数" inputType="number" error="{{carNumIsError}}" iptValue="{{car_num}}" unit="辆" data-event-opts="{{[['^inputBlur',[['inputBlur']]],['^updateIptValue',[['__set_sync',['$0','car_num','$event'],['']]]],['^updateIptValue',[['__set_sync',['$0','car_num','$event'],['']]]]]}}" bind:inputBlur="__e" bind:updateIptValue="__e"></km-input><km-title title="请选择装货时间"></km-title><km-select title="选择装货时间" icon="../static/time.png" error="{{startTimeIsError}}" text="{{start_time}}" navigateTo="../selectCalendar/selectCalendar"></km-select><km-title title="请填写货物信息"></km-title><km-select title="请填写货物名称" hasIcon="{{0}}" error="{{goodsNameIsError}}" text="{{goodsName}}" navigateTo="../selectGoodsName/selectGoodsName"></km-select><km-input title="请填写货物重量" iptValue="{{goodsWeight}}" error="{{goodsWeightIsError}}" inputType="digit" unit="吨" data-event-opts="{{[['^inputBlur',[['inputBlur']]],['^updateIptValue',[['__set_sync',['$0','goodsWeight','$event'],['']]]],['^updateIptValue',[['__set_sync',['$0','goodsWeight','$event'],['']]]]]}}" bind:inputBlur="__e" bind:updateIptValue="__e"></km-input><km-title title="请填写雇佣价格"></km-title><km-input title="请填写价格单位" iptValue="{{goodsMoney}}" error="{{goodsMoneyIsError}}" inputType="digit" unit="元/吨" data-event-opts="{{[['^inputBlur',[['inputBlur']]],['^updateIptValue',[['__set_sync',['$0','goodsMoney','$event'],['']]]],['^updateIptValue',[['__set_sync',['$0','goodsMoney','$event'],['']]]]]}}" bind:inputBlur="__e" bind:updateIptValue="__e"></km-input><km-input title="总运费" iptValue="{{goodsMoney*goodsWeight}}" unit="元" isDisabled="{{true}}"></km-input><km-title title="请填写联系人"></km-title><km-input title="请填写联系人姓名" iptValue="{{username}}" error="{{goodsUsernameIsError}}" data-event-opts="{{[['^inputBlur',[['inputBlur']]],['^updateIptValue',[['__set_sync',['$0','username','$event'],['']]]],['^updateIptValue',[['__set_sync',['$0','username','$event'],['']]]]]}}" bind:inputBlur="__e" bind:updateIptValue="__e"></km-input><textarea class="orderRemarks" placeholder-class="remarks_pla" placeholder="请填写订单备注" data-event-opts="{{[['input',[['__set_model',['$0','orderRemarks','$event',[]],['']]]]]}}" value="{{orderRemarks}}" bindinput="__e"></textarea><view data-event-opts="{{[['tap',[['releaseOrder']]]]}}" class="select_btn" bindtap="__e">发布</view></block><block wx:if="{{uid==''||uid==null}}"><view data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" class="select_btn" bindtap="__e">登录</view></block></view><uni-popup show="{{type==='middle-img'}}" position="middle" mode="insert" data-event-opts="{{[['^hidePopup',[['togglePopup',['']]]]]}}" bind:hidePopup="__e" vue-slots="{{['default']}}"><view class="uni-center center-box"><image class="image" src="/static/yhq.png"></image><view class="yhq">{{''+yhqMoney+'元优惠券'}}</view></view></uni-popup><uni-popup show="{{type==='middle'}}" position="middle" mode="fixed" data-event-opts="{{[['^hidePopup',[['togglePopup',['']]]]]}}" bind:hidePopup="__e" vue-slots="{{['default']}}"><view class="km_pop">选择信息与您绑定信息不符是否继续？</view><view class="pop_btns"><view data-event-opts="{{[['tap',[['togglePopup',['']]]]]}}" class="pop_btn cancle_btn" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['isSure',['$event']]]]]}}" class="pop_btn sure_btn sure_isAgree" bindtap="__e">确定</view></view></uni-popup></view>
.uni-swiper-tab{
	height: 180rpx;
	line-height: 0;
	border-bottom: solid 1px #EEEEEE;
}
.swiper-tab-list{
	width: 50%;
	font-size: 24.17rpx;
	color: #818181;
	height: 88rpx;
	line-height: 88rpx;
}
.swiper-tab-list.active{
	color: #139CFF;
	border-bottom: solid 2px #139CFF;
}
.swiper-box{
	height: calc(100% - 185rpx);
}
.list{
	height: 100%;
}
.order_filter{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	border-bottom: solid 1px #EEEEEE;
	height: 88rpx;
}
.filter{
	width: calc(100% / 3);
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	height: 100%;
	border-right: solid 1px rgba(19,156,255,0.25);
	color: #139cff;
	font-size: 24rpx;
}
.filter:last-child{
	border: none;
}
.list_null{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	width: 100%;
	padding-top: 60rpx;
}
.null_icon{
	width: 435rpx;
	height: 363rpx;
}
.null_text{
	color: #7c7c7c;
	font-size: 24rpx;
}
.recommend_title{
	color: #139CFF;
	font-size: 24rpx;
	height: 70rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding: 0 20rpx;
	border-bottom: solid 1px #EEEEEE;
	margin-top: 20rpx;
}
.recommend_icon{
	width: 24rpx;
	height: 32rpx;
	margin-right: 10rpx;
}


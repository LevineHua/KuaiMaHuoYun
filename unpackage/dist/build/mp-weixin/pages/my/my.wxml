<view class="page" bind:__l="__l"><view class="my_top"><view class="my_info"><block wx:if="{{uid!=''&&uid!=null}}"><view class="my_picture"><image class="picture" src="{{avatarUrl}}"></image></view><view class="my_name">{{nickname}}</view></block><block wx:if="{{uid==''||uid==null}}"><view data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" class="goLogin" bindtap="__e">去登陆</view></block><view class="census"><view data-event-opts="{{[['tap',[['goTab',[1]]]]]}}" class="census_item" bindtap="__e"><text class="item_status">待确认</text><text class="item_num">{{user.examine}}</text></view><view data-event-opts="{{[['tap',[['goTab',[2]]]]]}}" class="census_item" bindtap="__e"><text class="item_status">运输中</text><text class="item_num">{{user.transit}}</text></view><view data-event-opts="{{[['tap',[['goTab',[3]]]]]}}" class="census_item" bindtap="__e"><text class="item_status">已完成</text><text class="item_num">{{user.complete}}</text></view></view><view class="my_menu"><block wx:for="{{menus}}" wx:for-item="ms" wx:for-index="__i0__" wx:key="id"><block wx:if="{{ms.event!='fxlq'}}"><view data-event-opts="{{[['tap',[['clickMenu',['$0'],[[['menus','id',ms.id,'event']]]]]]]}}" class="menu_item" bindtap="__e"><image class="item_icon" src="{{ms.icon}}"></image>{{''+ms.title+''}}</view></block><block wx:if="{{ms.event=='fxlq'}}"><button class="menu_item" open-type="share"><image class="item_icon" src="{{ms.icon}}"></image>{{''+ms.title+''}}</button></block></block><block wx:if="{{userinfo.user_identity==1}}"><view data-event-opts="{{[['tap',[['clickMenu',['zlxg']]]]]}}" class="menu_item" bindtap="__e"><image class="item_icon" src="../../static/xgzl.png"></image>资料修改</view></block></view></view></view><uni-popup show="{{type==='lxpt'}}" position="bottom" mode="fixed" data-event-opts="{{[['^hidePopup',[['clickMenu',['']]]]]}}" bind:hidePopup="__e" vue-slots="{{['default']}}"><view class="call_item" data-phone="18507009330" data-event-opts="{{[['tap',[['makePhoneCall',['$event']]]]]}}" bindtap="__e">18507009330<image class="phone_icon" src="../../static/phone.png"></image></view><view data-event-opts="{{[['tap',[['clickMenu',['']]]]]}}" class="call_item" bindtap="__e">取消</view></uni-popup></view>